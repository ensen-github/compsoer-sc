# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
#字符集utf-8
charset = utf-8


# Indentation override for all JS under lib directory
[**.js]
indent_style = space
indent_size = 4
#行尾允许空格
trim_trailing_whitespace = true
#结尾总是插入新的一行
insert_final_newline = true

[**.cto]
indent_style = space
indent_size = 4
#行尾允许空格
trim_trailing_whitespace = true
#结尾总是插入新的一行
insert_final_newline = true
